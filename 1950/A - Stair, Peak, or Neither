#include<bits/stdc++.h>using namespace std;Â int main(){    long long t;    cin>>t;    while(t--)    {        long long a,b,c;        cin>>a>>b>>c;        if(a<b && b<c)        {            cout<<"STAIR\n";        }        else if(a<b && b>c)        {            cout<<"PEAK\n";        }        else        {            cout<<"NONE\n";        }    }}